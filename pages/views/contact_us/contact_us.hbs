<div class="container">
    <div class="row anchor-white-small" style="margin-top:81px;">
      <div class="anchor-col col-lg-12">
        Get Started on the ByteSize Cloud!
      </div>
    </div>
    <form id="email-form" enctype="multipart/form-data">
    <div class="form-group">
      <label for="email">Email address</label>
      <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="example@email.com">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
      <label for="name">First and last name</label>
      <input type="name" class="form-control" id="name" placeholder="John Doe">
    </div>
    <div class="form-group">
      <label for="restaurant-name">Restaurant name</label>
      <input type="name" class="form-control" id="restaurant-name" placeholder="Mi Rico Peru">
    </div>
    <div class="form-group">
      <label for="restaurant-name">Restaurant location</label>
      <input type="name" class="form-control" id="restaurant-name" placeholder="Fairfax, Virginia">
    </div> 
    <div class="form-group">
      <label for="phone">Phone Number</label>
      <input type="tel" class="form-control" id="phone" placeholder="(703) 555-5555">
    </div>
    <button class="btn btn-primary" onClick=sendEmail($('#email-form'))>Submit</button>
  </form>
</div>

